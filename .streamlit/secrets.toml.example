# Copy this file to secrets.toml and fill in your Snowflake credentials.
# This file is loaded automatically by Streamlit.
#
# Options:
#   password auth  → set password
#   PAT            → set authenticator = "PROGRAMMATIC_ACCESS_TOKEN" and token = "<your-pat>"
#   SSO            → set authenticator = "externalbrowser"

[snowflake]
account   = "your_account_identifier"      # e.g. "xy12345.us-east-1"
user      = "your_username"
password  = "your_password"                 # leave blank if using token/SSO
# authenticator = "PROGRAMMATIC_ACCESS_TOKEN"  # uncomment for PAT auth
# token    = "your_programmatic_access_token"
role      = "your_role"
warehouse = "your_warehouse"
database  = "DB_INSULINTEL"
schema    = "SCH_SEMANTIC"
